  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6a9955",
        "fontStyle": "italic"
      }
    },

    {
      "name": "String quotes",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#86d17a"
      }
    },
    {
      "name": "String content",
      "scope": [
        "string.quoted.single",
        "string.quoted.double",
        "string.quoted.triple"
      ],
      "settings": {
        "foreground": "#a6e3a1"
      }
    },
    {
      "name": "Template string quotes",
      "scope": [
        "punctuation.definition.string.template.begin",
        "punctuation.definition.string.template.end",
        "string.template punctuation.definition.string.begin",
        "string.template punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#76c76c"
      }
    },
    {
      "name": "Template string content",
      "scope": ["string.template", "string.quoted.template"],
      "settings": {
        "foreground": "#96d96b"
      }
    },
    {
      "name": "Template expression delimiters",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end"
      ],
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    {
      "name": "Template expression content",
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "#e1e5e9"
      }
    },
    {
      "name": "String escapes",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#66b366"
      }
    },
    {
      "name": "Regular expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#4ec9b0"
      }
    },

    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.numeric.decimal",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.binary",
        "constant.numeric.octal"
      ],
      "settings": {
        "foreground": "#79d4fd"
      }
    },

    {
      "name": "Keywords",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#ff7b54"
      }
    },
    {
      "name": "Control flow keywords",
      "scope": [
        "keyword.control",
        "keyword.control.flow",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.return",
        "keyword.control.throw",
        "keyword.control.try",
        "keyword.control.catch",
        "keyword.control.finally"
      ],
      "settings": {
        "foreground": "#ff6b35"
      }
    },
    {
      "scope": "storage.type.function",
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#e94c4c"
      }
    },
    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#ff49ad"
      }
    },
    {
      "name": "Storage keywords",
      "scope": ["keyword.other.new", "keyword.operator.new"],
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    {
      "name": "Import/Export keywords",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as",
        "keyword.control.default"
      ],
      "settings": {
        "foreground": "#ff54ff"
      }
    },

    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "keyword.operator.assignment",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "name": "Special operators",
      "scope": [
        "keyword.operator.typeof",
        "keyword.operator.instanceof",
        "keyword.operator.in",
        "keyword.operator.of",
        "keyword.operator.delete",
        "keyword.operator.void"
      ],
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    {
      "name": "Arrow function",
      "scope": ["storage.type.function.arrow"],
      "settings": {
        "foreground": "#ff6b35"
      }
    },

    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#4dd0e1"
      }
    },
    {
      "name": "TypeScript specific types",
      "scope": [
        "support.type.primitive",
        "support.type.builtin",
        "entity.name.type.ts",
        "entity.name.type.js"
      ],
      "settings": {
        "foreground": "#26c6da"
      }
    },
    {
      "name": "Interfaces",
      "scope": ["entity.name.type.interface"],
      "settings": {
        "foreground": "#00e5ff"
      }
    },
    {
      "name": "Generics",
      "scope": ["entity.name.type.parameter", "meta.type.parameters"],
      "settings": {
        "foreground": "#80deea"
      }
    },

    {
      "name": "Function names",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#ffd54f"
      }
    },
    {
      "name": "Function calls",
      "scope": ["variable.function", "meta.function-call"],
      "settings": {
        "foreground": "#ffcc02"
      }
    },
    {
      "name": "Method calls",
      "scope": ["meta.function-call.method"],
      "settings": {
        "foreground": "#ffe082"
      }
    },

    {
      "name": "Variables",
      "scope": ["variable.other.readwrite", "variable.other.object"],
      "settings": {
        "foreground": "#7db3d1"
      }
    },
    {
      "scope": "meta.function.parameters",
      "settings": {
        "foreground": "#a5c9e1"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#a5c9e1"
      }
    },
    {
      "name": "Constants",
      "scope": ["variable.other.constant", "variable.other.enummember"],
      "settings": {
        "foreground": "#4fc3f7"
      }
    },
    {
      "name": "Language variables",
      "scope": [
        "variable.language",
        "variable.language.this",
        "variable.language.super",
        "constant.language"
      ],
      "settings": {
        "foreground": "#ef5350",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Object properties",
      "scope": ["variable.object.property", "support.variable.property"],
      "settings": {
        "foreground": "#d4a5e8"
      }
    },
    {
      "name": "Object keys",
      "scope": ["meta.object-literal.key", "string.unquoted.label"],
      "settings": {
        "foreground": "#ce93d8"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#ba68c8"
      }
    },

    {
      "name": "JSX/TSX Tags",
      "scope": ["entity.name.tag.tsx", "entity.name.tag.jsx"],
      "settings": {
        "foreground": "#f48fb1"
      }
    },
    {
      "name": "JSX/TSX Component",
      "scope": ["support.class.component.tsx", "support.class.component.jsx"],
      "settings": {
        "foreground": "#ff4081"
      }
    },
    {
      "name": "JSX/TSX Attributes",
      "scope": [
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.jsx"
      ],
      "settings": {
        "foreground": "#ffc1cc"
      }
    },

    {
      "name": "HTML Tags",
      "scope": ["entity.name.tag.html"],
      "settings": {
        "foreground": "#ef5350"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": ["entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#90caf9"
      }
    },
    {
      "name": "Tag brackets",
      "scope": [
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "foreground": "#8892a0"
      }
    },

    {
      "name": "Brackets",
      "scope": [
        "punctuation.definition.block",
        "punctuation.definition.brackets",
        "punctuation.definition.parameters",
        "punctuation.definition.array"
      ],
      "settings": {
        "foreground": "#c9d1d9"
      }
    },

    {
      "name": "Decorators",
      "scope": ["punctuation.decorator", "meta.decorator"],
      "settings": {
        "foreground": "#e91e63"
      }
    },

    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#f44336"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable": "#7db3d1",
    "variable.readonly": "#4fc3f7",
    "variable.defaultLibrary": "#26c6da",
    "parameter": "#ffffff",

    "property": "#c7c7c7",
    "property.readonly": "#949494",

    "function": "#f0db4f",
    "function.defaultLibrary": "#ffcc02",
    "method": "#ffe082",

    "class": "#4dd0e1",
    "interface": "#00e5ff",
    "enum": "#26c6da",
    "type": "#4dd0e1",
    "typeParameter": "#80deea",
    "namespace": "#00e5ff",

    "enumMember": "#4fc3f7",
    "keyword": "#ff7b54",
    "string": "#a6e3a1",
    "number": "#79d4fd",
    "regexp": "#4ec9b0",
    "operator": "#d4d4d4",
    "comment": "#6a9955",
    "decorator": "#e91e63",
    "label": "#cbd5e0",

    "stringLiteral": "#a6e3a1",
    "numberLiteral": "#79d4fd",
    "customLiteral": "#ffcc02",
    "newOperator": "#ff8c42"
  }
}
